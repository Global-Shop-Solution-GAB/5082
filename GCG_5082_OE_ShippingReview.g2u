			f.Intrinsic.Control.If(v.Local.sNum.Trim,<>,"***NORETURN***")
				f.Data.DataTable.SetValue("dtDueShip",v.Local.sNum,"Priority",True)
				Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Local.sNum,"BackColor","LightGreen")			
			f.Intrinsic.Control.EndIf
		'True will turn False
		f.Intrinsic.Control.ElseIf(v.DataTable.dtAllShip(v.Args.RowIndex).Priority!FieldValString,=,"True")			
			f.Intrinsic.Math.Sub(v.Local.fQtyBO,v.DataTable.dtAllShip(v.Args.RowIndex).Qty_BO!FieldValString,v.Local.fQtyBO)
			f.Intrinsic.Math.Sub(v.DataTable.dtAllShip(v.Args.RowIndex).Qty_Onhand!FieldValString,v.Local.fQtyBO,v.Local.fAvai)
'			'Check if Available is still < 0
'			f.Intrinsic.Control.If(v.Local.fAvai,<,0)
'				f.Intrinsic.UI.Msgbox("Open Qty is greater than Available!","Invalid")
'				f.Intrinsic.UI.CloseWaitDialog
'				f.Intrinsic.Control.ExitSub
'			f.Intrinsic.Control.EndIf			
			f.Data.DataTable.SetValue("dtAllShip",v.Args.RowIndex,"Priority",False)
			Gui.frmShip.GsGCAllShip.setRowAppearance("gvAllShip",v.Args.RowIndex,"BackColor","transparent")
			f.Intrinsic.Control.CallSub("Update_Priority_DB","sOrderNo",v.DataTable.dtAllShip(v.Args.RowIndex).Order_No!FieldValString,"sRecordNo",v.DataTable.dtAllShip(v.Args.RowIndex).RecordNo!FieldValString,"Priority",0,"sDateDue",v.DataTable.dtAllShip(v.Args.RowIndex).Date_Due2!FieldValTrim,"sSeq",v.DataTable.dtAllShip(v.Args.RowIndex).Mult_Qty_Seq!FieldValTrim)
			'Set value for dueship grid
			f.Intrinsic.Control.If(v.Local.sNum.Trim,<>,"***NORETURN***")			
				f.Data.DataTable.SetValue("dtDueShip",v.Local.sNum,"Priority",False)
				Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Local.sNum,"BackColor","transparent")
			F.Intrinsic.Control.EndIf
		f.Intrinsic.Control.EndIf
		
		f.Intrinsic.Control.If(v.DataTable.dtAllShip(v.Args.RowIndex).SHP_HLD_FLAG!FieldValString,=,"True")
			Gui.frmShip.GsGCAllShip.SetRowAppearance("gvAllShip",v.Args.RowIndex,"BackColor","Pink")
			f.Intrinsic.Control.If(v.Local.sNum.Trim,<>,"***NORETURN***")
				Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Local.sNum,"BackColor","Pink")
			F.Intrinsic.Control.EndIf
		f.Intrinsic.Control.EndIf
		
		'Set Total_Priority for the specific Part, Location
		f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}'",v.DataTable.dtAllShip(v.Args.RowIndex).Part!FieldValString,v.DataTable.dtAllShip(v.Args.RowIndex).Location!FieldValString,v.Local.sFilter)
		'For dtAllShip
		f.Data.DataView.Create("dtAllShip","dvAllShipPartLoc",22,v.Local.sFilter,"")
