			F.Intrinsic.Control.CallSub(subPrint_PickList,"OrderNo",V.Local.sTemp,"ShipSchd",v.Local.pSHP)
			F.Intrinsic.Control.CallSub(updateprintflag,"Order",v.Local.sTemp)
		F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.Case("STAGED")
		F.Intrinsic.Control.If(V.Screen.frmShip!optShip.Value,=,True)
			F.Intrinsic.Control.AndIf(V.Args.CellValue,=,True)
			F.Global.General.CallWrapperSync(50,"SHP700/O")
		F.Intrinsic.Control.EndIf
		
	'BN, Nov 28 ''point
	F.Intrinsic.Control.Case("Priority")	
		f.Intrinsic.UI.InvokeWaitDialog("Processing Priority...")
		'working 2
		'Get the Num value to set data for the dueShip grid
		f.Intrinsic.String.Build("Num = {0}",v.DataTable.dtDueShip(v.Args.RowIndex).Num!FieldValTrim,v.Local.sFilter)
		f.Data.DataTable.Select("dtAllShip",v.Local.sFilter,v.Local.sNum)
		v.Local.iNum.Set(v.Local.sNum)
		
		f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}' and Priority = True",v.DataTable.dtAllShip(v.Local.iNum).Part!FieldValString,v.DataTable.dtAllShip(v.Local.iNum).Location!FieldValString,v.Local.sFilter)
		f.Data.DataTable.Compute("dtAllShip","Sum(Qty_BO)",v.Local.sFilter,v.Local.fQtyBO)
		'False will turn True
		f.Intrinsic.Control.If(v.DataTable.dtAllShip(v.Local.iNum).Priority!FieldValString,=,"False")			
			f.Intrinsic.Math.Add(v.Local.fQtyBO,v.DataTable.dtAllShip(v.Local.iNum).Qty_BO!FieldValString,v.Local.fQtyBO)
			f.Intrinsic.Math.Sub(v.DataTable.dtAllShip(v.Local.iNum).Qty_Onhand!FieldValString,v.Local.fQtyBO,v.Local.fAvai)
			'Check if Available is still < 0
			f.Intrinsic.Control.If(v.Local.fAvai,<,0)
				f.Intrinsic.UI.Msgbox("Open Qty is greater than Available!","Invalid")
				f.Intrinsic.UI.CloseWaitDialog
				f.Intrinsic.Control.ExitSub
			f.Intrinsic.Control.EndIf			
			f.Data.DataTable.SetValue("dtAllShip",v.Local.iNum,"Priority",True)
			Gui.frmShip.GsGCAllShip.setRowAppearance("gvAllShip",v.Local.iNum,"BackColor","LightGreen")
			f.Intrinsic.Control.CallSub("Update_Priority_DB","sOrderNo",v.DataTable.dtAllShip(v.Local.iNum).Order_No!FieldValString,"sRecordNo",v.DataTable.dtAllShip(v.Local.iNum).RecordNo!FieldValString,"Priority",1,"sDateDue",v.DataTable.dtAllShip(v.Local.iNum).Date_Due2!FieldValTrim,"sSeq",v.DataTable.dtAllShip(v.Local.iNum).Mult_Qty_Seq!FieldValTrim)
			'Set value for dueship grid
			f.Data.DataTable.SetValue("dtDueShip",v.Args.RowIndex,"Priority",True)
			Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Args.RowIndex,"BackColor","LightGreen")
		'True will turn False
		f.Intrinsic.Control.ElseIf(v.DataTable.dtAllShip(v.Local.iNum).Priority!FieldValString,=,"True")			
			f.Intrinsic.Math.Sub(v.Local.fQtyBO,v.DataTable.dtAllShip(v.Local.iNum).Qty_BO!FieldValString,v.Local.fQtyBO)
			f.Intrinsic.Math.Sub(v.DataTable.dtAllShip(v.Local.iNum).Qty_Onhand!FieldValString,v.Local.fQtyBO,v.Local.fAvai)
'			'Check if Available is still < 0
'			f.Intrinsic.Control.If(v.Local.fAvai,<,0)
'				f.Intrinsic.UI.Msgbox("Open Qty is greater than Available!","Invalid")
'				f.Intrinsic.UI.CloseWaitDialog
'				f.Intrinsic.Control.ExitSub
'			f.Intrinsic.Control.EndIf			
			f.Data.DataTable.SetValue("dtAllShip",v.Local.iNum,"Priority",False)
			Gui.frmShip.GsGCAllShip.setRowAppearance("gvAllShip",v.Local.iNum,"BackColor","transparent")
			f.Intrinsic.Control.CallSub("Update_Priority_DB","sOrderNo",v.DataTable.dtAllShip(v.Local.iNum).Order_No!FieldValString,"sRecordNo",v.DataTable.dtAllShip(v.Local.iNum).RecordNo!FieldValString,"Priority",0,"sDateDue",v.DataTable.dtAllShip(v.Local.iNum).Date_Due2!FieldValTrim,"sSeq",v.DataTable.dtAllShip(v.Local.iNum).Mult_Qty_Seq!FieldValTrim)
			'Set value for dueship grid
			f.Data.DataTable.SetValue("dtDueShip",v.Args.RowIndex,"Priority",False)
			Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Args.RowIndex,"BackColor","transparent")
		f.Intrinsic.Control.EndIf
		
		f.Intrinsic.Control.If(v.DataTable.dtAllShip(v.Local.iNum).SHP_HLD_FLAG!FieldValString,=,"True")
			Gui.frmShip.GsGCAllShip.SetRowAppearance("gvAllShip",v.Local.iNum,"BackColor","Pink")
			Gui.frmShip.GsGCDue.setRowAppearance("gvDueShip",v.Args.RowIndex,"BackColor","Pink")
		f.Intrinsic.Control.EndIf
		
		'Set Total_Priority for the specific Part, Location
		f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}'",v.DataTable.dtAllShip(v.Args.RowIndex).Part!FieldValString,v.DataTable.dtAllShip(v.Args.RowIndex).Location!FieldValString,v.Local.sFilter)
		'For dtAllShip
		f.Data.DataView.Create("dtAllShip","dvAllShipPartLoc",22,v.Local.sFilter,"")
		f.Data.DataView.SetValue("dtAllShip","dvAllShipPartLoc",-1,"Total_Priority",v.Local.fQtyBO,"Available",v.Local.fAvai)		
		f.Data.DataView.Close("dtAllShip","dvAllShipPartLoc")
		'For dtDueShip
		f.Data.DataView.Create("dtDueShip","dvDueShipPartLoc",22,v.Local.sFilter,"")
		f.Data.DataView.SetValue("dtDueShip","dvDueShipPartLoc",-1,"Total_Priority",v.Local.fQtyBO,"Available",v.Local.fAvai)		
		f.Data.DataView.Close("dtDueShip","dvDueShipPartLoc")
		f.Intrinsic.UI.CloseWaitDialog
		'==========================================================================================================================
F.Intrinsic.Control.EndSelect
	
	F.Intrinsic.Control.Catch
