			f.Intrinsic.Math.Sub(v.DataTable.dtAllShip(v.Args.RowIndex).Qty_Onhand!FieldValString,v.Local.fQtyBO,v.Local.fAvai)
			'Check if Available is still < 0
			f.Intrinsic.Control.If(v.Local.fAvai,<,0)
				f.Intrinsic.UI.Msgbox("Open Qty is greater than Available!","Invalid")
				f.Intrinsic.UI.CloseWaitDialog
				f.Intrinsic.Control.ExitSub
			f.Intrinsic.Control.EndIf			
			f.Data.DataTable.SetValue("dtAllShip",v.Args.RowIndex,"Priority",True)
			Gui.frmShip.GsGCAllShip.setRowAppearance("gvAllShip",v.Args.RowIndex,"BackColor","LightGreen")
			f.Intrinsic.Control.CallSub("Update_Priority_DB","sOrderNo",v.DataTable.dtAllShip(v.Args.RowIndex).Order_No!FieldValString,"sRecordNo",v.DataTable.dtAllShip(v.Args.RowIndex).RecordNo!FieldValString,"Priority",1,"sDateDue",v.DataTable.dtAllShip(v.Args.RowIndex).Date_Due2!FieldValTrim,"sSeq",v.DataTable.dtAllShip(v.Args.RowIndex).Mult_Qty_Seq!FieldValTrim)
