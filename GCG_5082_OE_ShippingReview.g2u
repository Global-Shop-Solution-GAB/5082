			F.Intrinsic.Control.CallSub(subPrint_PickList,"OrderNo",V.Local.sTemp,"ShipSchd",V.Local.pSHP)
			F.Intrinsic.Control.CallSub(updateprintflag,"Order",V.Local.sTemp)
		F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.Case("STAGED")
		F.Intrinsic.Control.If(V.Screen.frmShip!optShip.Value,=,True)
			F.Intrinsic.Control.AndIf(V.Args.CellValue,=,True)
			F.Global.General.CallWrapperSync(50,"SHP700/O")
		F.Intrinsic.Control.EndIf
		
	'BN, Nov 28 ''point
	F.Intrinsic.Control.Case("Priority")	
		f.Intrinsic.UI.InvokeWaitDialog("Processing Priority...")
		'working1
		'Get the Num value to set data for the dueShip grid
		f.Intrinsic.String.Build("Num = {0}",v.DataTable.dtAllShip(v.Args.RowIndex).Num!FieldValTrim,v.Local.sFilter)
		f.Data.DataTable.Select("dtDueShip",v.Local.sFilter,v.Local.sNum)
		
		f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}' and Priority = True",v.DataTable.dtAllShip(v.Args.RowIndex).Part!FieldValString,v.DataTable.dtAllShip(v.Args.RowIndex).Location!FieldValString,v.Local.sFilter)
