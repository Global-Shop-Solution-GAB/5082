Program.Sub.Get_Total_Priority_and_Available.Start
'Calculate Total Priority qty and then Available qty for all parts in the dashboard
f.Intrinsic.Control.Try
	v.Local.sError.Declare
	v.Local.iCnt.Declare
	v.Local.sFilter.Declare
	v.Local.fQtyBO.Declare
	v.Local.sPartLoc.Declare
	v.Local.fAvai.Declare
	v.Local.sPart.Declare
	
	'Dictionary (PartLoc,TotalPriority)
	f.Data.Dictionary.Create("TotalPrDict")
	f.Data.Dictionary.SetDefaultReturn("TotalPrDict",0)
	'Dictionary (PartLoc,TotalAvailable)
	f.Data.Dictionary.Create("AvaiDict")
	f.Data.Dictionary.SetDefaultReturn("AvaiDict",0)
	
	f.Data.DataView.ToDataTableDistinct("dtAllShip","dvAllShip","distinctPartLoc","Part*!*Location*!*Qty_Onhand",True)
	f.Intrinsic.Control.For(v.Local.iCnt,0,v.DataTable.distinctPartLoc.RowCount--,1)
		f.Intrinsic.String.Replace(v.DataTable.distinctPartLoc(v.Local.iCnt).Part!FieldValString,"'","",v.Local.sPart)
		f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}' and Priority = True",v.Local.sPart,v.DataTable.distinctPartLoc(v.Local.iCnt).Location!FieldValString,v.Local.sFilter)
		f.Data.DataTable.Compute("dtAllShip","Sum([Qty_BO])",v.Local.sFilter,v.Local.fQtyBO)
		f.Intrinsic.String.Build("{0}-{1}",v.DataTable.distinctPartLoc(v.Local.iCnt).Part!FieldValString,v.DataTable.distinctPartLoc(v.Local.iCnt).Location!FieldValString,v.Local.sPartLoc)
		f.Data.Dictionary.AddItem("TotalPrDict",v.Local.sPartLoc,v.Local.fQtyBO)
		f.Intrinsic.Math.Sub(v.DataTable.distinctPartLoc(v.Local.iCnt).Qty_Onhand!FieldValString,v.Local.fQtyBO,v.Local.fAvai)
		f.Data.Dictionary.AddItem("AvaiDict",v.Local.sPartLoc,v.Local.fAvai)
	f.Intrinsic.Control.Next(v.Local.iCnt)
	
	f.Data.DataTable.FillFromDictionary("dtAllShip","TotalPrDict","PartLoc","Total_Priority")
	f.Data.DataTable.FillFromDictionary("dtAllShip","AvaiDict","PartLoc","Available")
	
	f.Data.Dictionary.Close("TotalPrDict")
	f.Data.Dictionary.Close("AvaiDict")
	f.Data.DataTable.Close("distinctPartLoc")

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}GAB Version: {5}}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	f.Intrinsic.Control.End
F.Intrinsic.Control.EndTry
Program.Sub.Get_Total_Priority_and_Available.End


Program.Sub.GsGCAllShip_RowCellClick.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
